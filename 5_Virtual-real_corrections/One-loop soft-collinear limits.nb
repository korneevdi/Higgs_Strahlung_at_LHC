Cell[BoxData[
 RowBox[{
  RowBox[{
   SubscriptBox["P", 
    RowBox[{"qq", ",", "loop", ",", "i"}]], "=", 
   RowBox[{
    RowBox[{"-", 
     FractionBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{"z", "+", "\[Epsilon]", "-", 
         RowBox[{"z", " ", "\[Epsilon]"}]}], ")"}], " ", 
       SubscriptBox["c", "\[CapitalGamma]"], " ", 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["C", "A"], "-", 
         SubscriptBox["C", "F"]}], ")"}], " ", 
       SubscriptBox["C", "F"], " ", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"-", 
          FractionBox["1", 
           SubscriptBox["s", "14"]]}], ")"}], "\[Epsilon]"]}], 
      RowBox[{"1", "-", 
       RowBox[{"2", " ", "\[Epsilon]"}]}]]}], "+", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", " ", 
        SubscriptBox["c", "\[CapitalGamma]"], " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", 
              FractionBox[
               RowBox[{
                SuperscriptBox[
                 RowBox[{"(", 
                  FractionBox["1", 
                   RowBox[{"1", "-", "z"}]], ")"}], "\[Epsilon]"], " ", 
                SuperscriptBox[
                 RowBox[{"Gamma", "[", 
                  RowBox[{"1", "-", "\[Epsilon]"}], "]"}], "2"], " ", 
                SuperscriptBox[
                 RowBox[{"Gamma", "[", 
                  RowBox[{"1", "+", "\[Epsilon]"}], "]"}], "2"]}], 
               RowBox[{"2", " ", 
                SuperscriptBox["\[Epsilon]", "2"], " ", 
                RowBox[{"Gamma", "[", 
                 RowBox[{"1", "-", 
                  RowBox[{"2", " ", "\[Epsilon]"}]}], "]"}], " ", 
                RowBox[{"Gamma", "[", 
                 RowBox[{"1", "+", 
                  RowBox[{"2", " ", "\[Epsilon]"}]}], "]"}]}]]}], "-", 
             RowBox[{"PolyLog", "[", 
              RowBox[{"2", ",", 
               RowBox[{"1", "-", "z"}]}], "]"}]}], ")"}], " ", 
           SubscriptBox["C", "A"]}], "+", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", 
