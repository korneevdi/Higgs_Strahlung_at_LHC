Cell[TextData[{
 "The collinear operator for splitting from the second quark can be obtained \
by exchanging the momenta 1 and 2:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["C", "42"], 
     RowBox[{
      SubscriptBox["F", "LRV"], "(", 
      RowBox[{
       SubscriptBox["1", "q"], ",", 
       RowBox[{
        SubscriptBox["2", "q"], "|", 
        SubscriptBox["4", "g"]}]}], ")"}]}], "=", 
    RowBox[{
     FractionBox[
      SubsuperscriptBox["g", 
       RowBox[{"s", ",", "b"}], "2"], 
      RowBox[{"-", 
       RowBox[{
        SubscriptBox["p", "2"], ".", 
        SubscriptBox["p", "4"]}]}]], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{
         SubsuperscriptBox["P", "qq", 
          RowBox[{
           RowBox[{"(", "0", ")"}], ",", "i"}]], "(", "z", ")"}], 
        FractionBox[
         RowBox[{
          SubscriptBox["F", "LV"], "(", 
          RowBox[{
           SubscriptBox["1", "q"], ",", 
           RowBox[{"z", 
            SubscriptBox[".2", "q"]}]}], ")"}], "z"]}], "+", 
       RowBox[{
        RowBox[{"[", 
         SubscriptBox["\[Alpha]", "s"], "]"}], 
        SuperscriptBox[
         RowBox[{
          SubscriptBox["c", "\[CapitalGamma]"], "(", 
          FractionBox["1", 
           RowBox[{"2", 
            RowBox[{
             SubscriptBox["p", "2"], ".", 
             SubscriptBox["p", "4"]}]}]], ")"}], "\[Epsilon]"], 
        RowBox[{
         SubsuperscriptBox["P", "qq", 
          RowBox[{"loop", ",", "i"}]], "(", "z", ")"}], 
        FractionBox[
         RowBox[{
          SubscriptBox["F", "LM"], "(", 
          RowBox[{
           SubscriptBox["1", "q"], ",", 
           RowBox[{"z", 
            SubscriptBox[".2", "q"]}]}], ")"}], "z"]}]}], " ", ")"}]}]}], 
   TraditionalForm]],ExpressionUUID->"4f604d96-4454-4abb-ae4b-58de3cf57752"],
 "\nwith\n ",
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SubscriptBox["E", "2"], "-", 
       SubscriptBox["E", "4"]}], 
      SubscriptBox["E", "2"]], "\[Element]", 
     RowBox[{"[", 
      RowBox[{"0", ",", "1"}], "]"}]}]}], TraditionalForm]],ExpressionUUID->
  "174500cb-4693-4323-8a65-162445886d21"],
 ".\n [Note: In ",
 Cell[BoxData[
  RowBox[{
   SubsuperscriptBox["P", "qq", 
    RowBox[{"loop", ",", "i"}]], 
   RowBox[{"(", "z", ")"}]}]],ExpressionUUID->
  "f710a732-406d-49c6-ba12-bc26a4298429"],
 " one has also to replace ",
 Cell[BoxData[
  FormBox[
