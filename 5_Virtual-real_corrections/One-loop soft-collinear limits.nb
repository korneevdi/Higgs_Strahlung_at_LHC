Cell[TextData[{
 "The collinear operator acts on the real-virtual amplitude as follows:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["C", "41"], 
     RowBox[{
      SubscriptBox["F", "LRV"], "(", 
      RowBox[{
       SubscriptBox["1", "q"], ",", 
       RowBox[{
        SubscriptBox["2", "q"], "|", 
        SubscriptBox["4", "g"]}]}], ")"}]}], "=", 
    RowBox[{
     FractionBox[
      SubsuperscriptBox["g", 
       RowBox[{"s", ",", "b"}], "2"], 
      RowBox[{"-", 
       RowBox[{
        SubscriptBox["p", "1"], ".", 
        SubscriptBox["p", "4"]}]}]], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{
         SubsuperscriptBox["P", "qq", 
          RowBox[{
           RowBox[{"(", "0", ")"}], ",", "i"}]], "(", "z", ")"}], 
        FractionBox[
         RowBox[{
          SubscriptBox["F", "LV"], "(", 
          RowBox[{
           RowBox[{"z", 
            SubscriptBox[".1", "q"]}], ",", 
           SubscriptBox["2", "q"]}], ")"}], "z"]}], "+", 
       RowBox[{
        RowBox[{"[", 
         SubscriptBox["\[Alpha]", "s"], "]"}], 
        SuperscriptBox[
         RowBox[{
          SubscriptBox["c", "\[CapitalGamma]"], "(", 
          FractionBox["1", 
           RowBox[{"2", 
            RowBox[{
             SubscriptBox["p", "1"], ".", 
             SubscriptBox["p", "4"]}]}]], ")"}], "\[Epsilon]"], 
        RowBox[{
         SubsuperscriptBox["P", "qq", 
          RowBox[{"loop", ",", "i"}]], "(", "z", ")"}], 
        FractionBox[
         RowBox[{
          SubscriptBox["F", "LM"], "(", 
          RowBox[{
           RowBox[{"z", 
            SubscriptBox[".1", "q"]}], ",", 
           SubscriptBox["2", "q"]}], ")"}], "z"]}]}], " ", ")"}]}]}], 
   TraditionalForm]],ExpressionUUID->"98e341e8-9525-486e-b9af-0e50521eabef"],
 "\nwith\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"[", 
     SubscriptBox["\[Alpha]", "s"], "]"}], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SubscriptBox["\[Alpha]", "s"], "(", "\[Mu]", ")"}], 
      RowBox[{"2", "\[Pi]"}]], 
     FractionBox[
      RowBox[{
       SuperscriptBox["\[Mu]", 
        RowBox[{"2", "\[Epsilon]"}]], 
       SuperscriptBox["e", 
        SubscriptBox["\[Epsilon]\[Gamma]", "E"]]}], 
      RowBox[{"\[CapitalGamma]", "(", 
       RowBox[{"1", "-", "\[Epsilon]"}], ")"}]]}]}], TraditionalForm]],
  ExpressionUUID->"6164424b-1425-466a-a0d3-eb202ec02698"],
 ",\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["c", "\[CapitalGamma]"], "=", 
    FractionBox[
     RowBox[{
      RowBox[{
       SuperscriptBox["\[CapitalGamma]", "3"], "(", 
       RowBox[{"1", "-", "\[Epsilon]"}], ")"}], 
      RowBox[{"\[CapitalGamma]", "(", 
       RowBox[{"1", "+", "\[Epsilon]"}], ")"}]}], 
     RowBox[{"\[CapitalGamma]", "(", 
      RowBox[{"1", "-", 
       RowBox[{"2", "\[Epsilon]"}]}], ")"}]]}], TraditionalForm]],
  ExpressionUUID->"246bc94a-ad61-4c9c-b006-86326de4c1ba"],
 "\nand\n ",
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SubscriptBox["E", "1"], "-", 
       SubscriptBox["E", "4"]}], 
      SubscriptBox["E", "1"]], "\[Element]", 
     RowBox[{"[", 
      RowBox[{"0", ",", "1"}], "]"}]}]}], TraditionalForm]],ExpressionUUID->
  "77f66709-95d1-454c-b04d-8959b93427bd"],
 "."
}], "Text",
 CellChangeTimes->{{3.852898062709097*^9, 3.85289836177949*^9}, {
